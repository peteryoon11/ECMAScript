<script type="text/javascript">
class Employee
{
 	constructor(name,salary)
	{
		this.name=name;
		this.salary=salary;
	} 
	setName(name)
	{
		this.name=name;
		
	}
	setSalary(salary)
	{
		this.salary=salary;
	}
	getName()
	{
		return this.name;
	}
	getSalary()
	{
		return this.salary;
	}
	getEmployee()
	{
		return this.name+"\t"+this.salary;
	}
	
}// end Employee class

class Manager extends Employee
{
	/* constructor(name,salary,depart)
	{
	super(name,salary);
	this.depart=depart;
	} */
	getEmployee()
	{
		return super.getEmployee()+"\t"+this.depart;
	}
}

let manager=new Manager("이릉이름",3000,"depart!!");
	console.log(manager);
	console.log(manager.getEmployee());
	
	
</script>
