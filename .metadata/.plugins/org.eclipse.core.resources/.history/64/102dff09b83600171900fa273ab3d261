<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
// 1. 


let y="hello";
for (let i of y)
	{
	// 실제값전송 인덱스 값이 아니라 
	console.log(i);
	}
	
	let yy={"dd":"dd1","dd2"
			:"dd22"};
	// 객체는 for in 은 되지만 for of 는 안됨 
	// iterable 속성을 가지고 있지 않기 때문에. 
	for (let i in yy)
		{
		console.log(i);
		}
	
	for (let i of yy)
	{
	console.log(i);
	}
</script>
</head>
<body>

</body>
</html>