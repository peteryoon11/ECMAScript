<script type="text/javascript">

function aa(a,...b){
	console.log(a,"  ",b,">>" , arguments);
	for(let i in b)
		{
		console.log(i," ", b[i]);
		}
	console.log("----");
}
aa();
aa(100);
aa(100,200);
aa(8,4,3);

function bb(n,m,...k){
//	console.log(a,"  ",b,">>" , arguments);
console.log(n,m,k);
	for(let i in k)
		{
		console.log(i," ", k[i]);
		}
	console.log("----");
}
bb();
bb(100);
bb(100,200);
bb(8,4,3);

</script>
