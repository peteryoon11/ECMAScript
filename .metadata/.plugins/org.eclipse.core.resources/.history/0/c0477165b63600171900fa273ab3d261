<script type="text/javascript">
// 1. 

var person={name:"홍길동",
		"age":20,
		setName: function (n){
			this.name=n;	
		},
		getName: function (n){
			return this.name;	
		},
		setAge: function (n){
			this.age=n;	
		},
		getAge: function (n){
			return this.age;	
		}
		
};
person.setName("이순신");
	
	console.log(person.getName()+"\t"
			+person.getAge());
	
</script>
