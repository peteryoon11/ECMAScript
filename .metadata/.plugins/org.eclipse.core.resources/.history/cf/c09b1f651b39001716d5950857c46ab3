<script type="text/javascript">
class Employee
{
 	constructor(name,salary)
	{
		this.name=name;
		this.salary=salary;
	} 
	setName(name)
	{
		this.name=name;
		
	}
	setSalary(salary)
	{
		this.salary=salary;
	}
	getName()
	{
		return this.name;
	}
	getSalary()
	{
		return this.salary;
	}
	getEmployee()
	{
		return this.name+"\t"+this.salary;
	}
	
	getInfo()
	{
		return this.getName()+"\t"+this.getName;
	}
}// end Employee class

class Manager extends Employee
{
	/* constructor(name,salary,depart)
	{
	super(name,salary);
	this.depart=depart;
	} */
	getEmployee()
	{
		return super.getEmployee()+"\t"+this.depart;
	}
	getName()
	{
		return "Manager 의 name "+this.name;
	}
	getInfo()
	{
		return super.getInfo()+"\t"+this.getDepart();
	}
}

let manager=new Manager("이릉이름",3000,"depart!!");
let employee=new Manager("이릉이름1212",5000,"depart!!");
	console.log(manager);
	console.log(manager.getName());
	console.log(manager.getEmployee());
	console.log(employee);
	
