<script type="text/javascript">

// 1. 일반적인 함수
var aa=function(x){
	return x+10;
}
console.log(aa(11));

// 2. 람다 함수 
var aa2 =(x)=>{
	return x+10;
};
console.log(aa2(11));

var aa3 = x=>
{
	return x+10;
	};
	
	console.log(aa3(11));
	// 인자가 여러개면 () 생략이 안됨 
	var d = x=> x+10;
	
	d(1,2);
	
	var d2 = (x,y)=>{
		console.log(x,y);
		
	};
	
	
	
</script>
