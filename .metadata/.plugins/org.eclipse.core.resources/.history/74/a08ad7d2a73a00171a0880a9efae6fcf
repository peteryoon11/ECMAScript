<script type="text/javascript">
class Person{
	var test=22;
	setName(name)
	{
		this.name=name;
	}
	setAge(age)
	{
		this.age=age;
	}
	getName()
	{
		return this.name;
	}
	getAge()
	{
		return this.age;
	}
	//public Person()
/* 	{
		//super();
		console.log("Contructor!!");
	} */
	//this.__proto__;
}
	
	var p = new Person();
	p.setName("홍길동");
	p.setAge(30);
	console.log(p);
	console.log(p.__proto__);
	console.log(p.__proto__.contructor);
	console.log(p.__proto__.getAge());
	let p2 = new Person();
	console.log(p2);


</script>
