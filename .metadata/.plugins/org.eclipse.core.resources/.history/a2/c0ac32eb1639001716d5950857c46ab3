<title>Insert title here</title>
<script type="text/javascript">
class PersonV{
	
 // 생성자 
	constructor(name,age,test)
	{
		console.log("contructor!!");
		this.name=name;
		this.age=age;
		console.log("contructor!! end");
	}

	set setName(name)
	{
		this.name=name;
	}
	set setAge(age)
	{
		this.age=age;
	}
	get getName()
	{
		return this.name;
	}
	get getAge()
	{
		return this.age;
	}
	//public Person()
/* 	{
		//super();
		console.log("Contructor!!");
	} */
	//this.__proto__;
}
	
	var p = new PersonV();
	p.setName="홍길동";
	p.setAge=30;
	console.log(p);
	console.log(p.__proto__);
	console.log(p.__proto__.contructor);
	console.log(p.__proto__.getAge);
	console.log(p.getName,p.getAge);
	
	let p2 = new PersonV();
	console.log(p2);
	
	let p3 = new PersonV("생성자",33);
	//p3.setAge=33;
	console.log(p3);
	
	
</script>
