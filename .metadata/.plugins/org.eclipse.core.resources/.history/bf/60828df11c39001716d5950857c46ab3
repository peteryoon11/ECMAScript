<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
class Employee
{
	static var cc="";

	constructor(v)
	{
		this.v=v;
		this.cc=v;
	}

	getEmployee()
	{
		return "hello "+this.v;
	}
	static getXXX()
	{
		return "hello2 "+this.v+"\t"+this.cc;
	}
	
}
	
	console.log(Employee.getXXX());
	
	var emp= new Employee(34);
	
	console.log(emp.getEmployee());
	console.log(Employee.getXXX());
	
	
</script>
</head>
