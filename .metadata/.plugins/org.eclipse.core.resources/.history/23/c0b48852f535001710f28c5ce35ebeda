<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
// 1. 일반적인 경우 
var x ={name:"홍길동 ",age:20};
var x2 =x['name'];
var x3 = x['age'];
console.log(x,x2,x3);
console.log(x.name, x.age);
// 1. destructuring 
let {name,age} = {name:"홍길동",age:20};

console.log(name,age);

let y, y2;
({y,y2}={y:"홍길동",y2:20});
console.log(y,y2);

let z,z2,z3;
({z,z2,z3="zzz"}={z:"홍길동",z2:20});// 기본값 
console.log(z,z2,z3);

// 변수 이름 바꾸기 
let kk,kk2;
//({kk,kk2}={x:100,y:200}); // 변수 명이 다르기 때문에
( { x1 : kk ,y1 : kk2 }={ x1 : 100 ,y1 : 200 } ) ;
console.log(kk,kk2); 


var ppp ={n:100, n2:200, n3 :{m:300,m2:400}};

({n:xx,n2:xx2,n3:{xx4,xx5}}=ppp);
console.log(xx,xx2,xx3);

</script>
</head>
<body>

</body>
</html>