<script type="text/javascript">
var PersonV=class {
	set setName(name)
	{
		this.name=name;
	}
	set setAge(age)
	{
		this.age=age;
	}
	get getName()
	{
		return this.name;
	}
	get getAge()
	{
		return this.age;
	}
	//public Person()
/* 	{
		//super();
		console.log("Contructor!!");
	} */
	//this.__proto__;
}
	
	var p = new PersonV();
	p.setName="홍길동";
	p.setAge=30;
	console.log(p);
	console.log(p.__proto__);
	console.log(p.__proto__.contructor);
	console.log(p.__proto__.getAge);
	console.log(p.getName,p.getAge);
	let p2 = new PersonV();
	console.log(p2);


</script>
